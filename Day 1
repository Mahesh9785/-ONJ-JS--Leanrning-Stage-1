var myArray = ["What is the meaning of life?",42,true];
console.log(myArray[0]);
console.log(myArray[1]);
console.log(myArray[2]);

var myArray = [true, "What is the meaning of life?"];
myArray.push(42);
myArray.shift();

console.log(myArray[0]);
console.log(myArray[1]);

