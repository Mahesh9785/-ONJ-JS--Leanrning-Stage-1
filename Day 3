function binary(decimal) {
let result="";
	do{
		if(decimal%2==0){
		result+=0;
		decimal/=2;
	}else{
			result+=1;
		decimal=decimal-1;
				decimal/=2;
	}
	}while(decimal>0)
	var splitString = result.split("");
	var reverseArray = splitString.reverse();
	var joinArray = reverseArray.join("");
		return joinArray;
}

